<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <title>STRABORN | @ViewData["Title"]</title>
    <!--boostrab css -->
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome Icons -->


    <link href="~/lib/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="~/lib/fontawesome-free/css/fontawesome.min.css" rel="stylesheet" />
    <link href="~/lib/fontawesome-free/css/solid.min.css" rel="stylesheet" />
    <link href="~/lib/fontawesome-free/css/brands.min.css" rel="stylesheet" />
    <!-- overlayScrollbars -->
    <link href="~/lib/dropzone/dropzone.min.css" rel="stylesheet" />
    <link href="~/lib/overlayScrollbars/css/OverlayScrollbars.css" rel="stylesheet" />
    <!-- Theme style -->

    <link href="~/css/adminlte.css" rel="stylesheet" />
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">

    @*toaster*@
    <link href="~/lib/toastr-2.1.4/build/toastr.css" rel="stylesheet" />

    @*sweetalert2*@
    <link href="~/lib/sweetalert2/sweetalert2.css" rel="stylesheet" />

    @*select2*@
    <link href="~/lib/select2/select2.css" rel="stylesheet" />

    @*datatables*@
    <link href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/responsive.dataTables.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/autoFill.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/buttons.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/colReorder.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/fixedColumns.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/fixedHeader.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/keyTable.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/responsive.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/rowGroup.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/rowReorder.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/select.dataTables.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/searchPanes.dataTables.min.css" rel="stylesheet" />
</head>
<body class="hold-transition sidebar-mini layout-fixed layout-navbar-fixed layout-footer-fixed">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" onclick="pushMenuButton()" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a asp-action="Index" asp-controller="Order" class="nav-link">Siparişler</a>
                </li>
              
                    <li class="nav-item d-none d-sm-inline-block">
                        <a asp-action="Index" asp-controller="Product" class="nav-link">Ürünler</a>
                    </li>
                
            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Messages Dropdown Menu -->
                <li class="nav-item dropdown">
                    <a class="nav-link" asp-action="Logout" asp-controller="Security">
                        <i class="fas fa-sign-out-alt"></i>
                    </a>

                </li>
                <!-- Notifications Dropdown Menu -->


            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-blue elevation-4">
            <!-- Brand Logo -->
            <a asp-action="Index" asp-controller="Home" class="brand-link">
                <img src="~/assets/templateImage/logo.jpeg" alt="AdminLTE Logo" class="brand-image elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">STRABORN</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->

                        <li class="nav-item">
                            <a asp-controller="Order" asp-action="Index" class="nav-link">
                                <i class="fas fa-shopping-basket"></i>
                                <p>
                                    Siparişler
                                </p>
                            </a>
                        </li>
                            <li class="nav-item">
                                <a asp-controller="Product" asp-action="Index" class="nav-link">
                                    <i class="nav-icon fas fa-th"></i>
                                    <p>
                                        Ürünler
                                    </p>
                                </a>
                            </li>

                            <li class="nav-item has-treeview">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fas fa-copy"></i>
                                    <p>
                                        Tanımlar
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a asp-controller="Definitions" asp-action="AdressDefinations" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Adres Tanımları</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Definitions" asp-action="PeriotDefination" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Zaman Tanımları</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Definitions" asp-action="Category" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Kategori Tanımları</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Gallery" asp-action="Index" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Galeri</p>
                                        </a>
                                    </li>

                                </ul>
                            </li>
                            <li class="nav-item has-treeview">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fas fa-chart-pie"></i>
                                    <p>
                                        Admin
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a asp-controller="user" asp-action="AdminUsers" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Sistam Kullanıcıları</p>
                                        </a>
                                    </li>
                                </ul>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a asp-controller="Order" asp-action="OrderReports" class="nav-link">
                                            <i class="fas fa-chart-pie"></i>
                                            <p>Sipariş Raporları</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>

                        


                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <div class="content-wrapper">

            <section class="content">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">

                            <h2 style="padding-left:10px" class="m-0 text-dark">@ViewData["Title"]</h2>
                        </div><!-- /.col -->

                    </div>

                    @RenderBody()


                </div>
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
        </aside>


    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->

    <script src="~/lib/jquery/jquery.js"></script>



    <!-- Bootstrap -->

    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- overlayScrollbars -->

    <script src="~/lib/overlayScrollbars/js/jquery.overlayScrollbars.js"></script>
    <!-- AdminLTE App -->

    <script src="~/js/adminlte.js"></script>

    <!-- OPTIONAL SCRIPTS -->

    <script src="~/js/adminlte.js"></script>

    <!-- PAGE PLUGINS -->
    <!-- jQuery Mapael -->

    <script src="~/lib/jquery-mousewheel/jquery.mousewheel.js"></script>

    <script src="~/lib/raphael/raphael.js"></script>

    <script src="~/lib/jquery-mapael/jquery.mapael.js"></script>

    <script src="~/lib/jquery-mapael/maps/usa_states.js"></script>
    <!-- ChartJS -->

    <script src="~/lib/chart.js/Chart.js"></script>

    <!-- PAGE SCRIPTS -->

    <script src="~/js/dashboard2.js"></script>

    @*momentjs*@
    <script src="~/lib/momentjs/moment-with-locales.js"></script>

    @*datatables*@
    <script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.responsive.min.js"></script>
    <script src="~/lib/datatables/js/responsive.bootstrap4.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.buttons.min.js"></script>
    <script src="~/lib/datatables/js/buttons.bootstrap4.min.js"></script>
    <script src="~/lib/datatables/js/buttons.colVis.min.js"></script>
    <script src="~/lib/datatables/js/buttons.flash.min.js"></script>
    <script src="~/lib/datatables/js/buttons.html5.min.js"></script>
    <script src="~/lib/datatables/js/buttons.print.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.autoFill.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.colReorder.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.fixedColumns.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.fixedHeader.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.keyTable.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.rowGroup.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.rowReorder.min.js"></script>
    <script src="~/lib/datatables/js/autoFill.bootstrap4.min.js"></script>
    <script src="~/lib/momentjs/datetime-moment.js"></script>
    <script src="~/lib/datatables/js/dataTables.select.min.js"></script>
    <script src="~/lib/JSZip/jszip.js"></script>
    <script src="~/lib/datatables/js/dataTables.searchPanes.min.js"></script>
    <script src="~/lib/datatables/js/datetime-moment.js"></script>
    <script src="~/lib/datatables/js/datetime.js"></script>
    <script src="~/lib/datatables/js/ellipsis.js"></script>

    @*select2*@
    <script src="~/lib/select2/select2.js"></script>
    <script src="~/lib/select2/tr.js"></script>

    @*toaster*@
    <script src="~/lib/toastr-2.1.4/toastr.js"></script>
    <script src="~/lib/toastr-2.1.4/build/toastr.min.js"></script>

    <!--SignalR WepSocet-->
    <script src="~/lib/signalr/dist/browser/signalr.js"></script>



    @*sweetAlert*@
    <script src="~/lib/sweetalert2/sweetalert2.min.js"></script>
    @*blockUI*@
    <script src="~/lib/jquery.blockUI/jquery.blockUI.js"></script>
    @*dropzone*@
    <script src="~/lib/dropzone/dropzone.min.js"></script>
    <script src="~/js/pages/basejses/baseProps.js"></script>

    <script type="text/javascript">$('.slc2').select2({ width: '100%' });</script>



    @RenderSection("Scripts", required: false)

</body>
</html>
